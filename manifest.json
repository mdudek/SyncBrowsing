{
    "manifest_version": 2,
    "name": "SyncBrowsing",
    "version": "1.1",
    "description": "Synchronized browsing extension",
    "background": {
      "page": "background.html"
    },
    "options_page": "options.html",
    "icons": {
        "16": "images/SyncBrowsing-19.png",
        "48": "images/SyncBrowsing-48.png",
        "128": "images/SyncBrowsing-128.png"
    },
    "browser_action": {
        "default_icon": "images/SyncBrowsing-19.png",
        "default_title": "SyncBrowsing",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs"],
    "web_accessible_resources": [
        "jquery-1.10.2.min.js",
        "syncbrowsing.js"
    ]
}
